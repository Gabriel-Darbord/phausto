"
A collection of Envelopes from the Faust envelopes.lib
"
Class {
	#name : #Reverbs,
	#superclass : #PhBox,
	#instVars : [
		'label'
	],
	#category : #'Phausto-BoxAPI-Effects'
}

{ #category : #accessing }
Reverbs >> label [

	^ label
]

{ #category : #accessing }
Reverbs >> label: aString [

	label := aString
]
